<div class="form-actions">
  <% if Hyrax.config.analytics? %>
    <%= link_to "Analytics", @presenter.stats_path, id: 'stats', class: 'btn btn-sm btn-default' %>
  <% end %>

  <% if @presenter.editor? %>
      <%= link_to "Edit This #{@presenter.human_readable_type}", edit_polymorphic_path([main_app, @presenter]),
                  class: 'btn btn-sm btn-default' %>
      <%= link_to "Delete This #{@presenter.human_readable_type}", [main_app, @presenter],
                  class: 'btn btn-sm btn-danger', data: { confirm: "Delete this #{@presenter.human_readable_type}?" },
                  method: :delete %>
  <% end %>

  <%# render 'social_media' %>
</div>
